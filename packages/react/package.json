{
  "name": "@a13y/react",
  "version": "0.1.3",
  "type": "module",
  "description": "Runtime-enforced React hooks and components for accessibility",
  "license": "MIT",
  "author": {
    "name": "Diego Aneli",
    "url": "https://github.com/DiegoAneli"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DiegoAneli/a13y.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/DiegoAneli/a13y/issues"
  },
  "homepage": "https://a13y.dev",
  "keywords": [
    "accessibility",
    "a11y",
    "react",
    "hooks",
    "components",
    "wcag",
    "aria",
    "focus-trap",
    "keyboard-navigation",
    "typescript",
    "type-safe"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "default": "./dist/hooks/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "default": "./dist/components/index.js"
    },
    "./patterns": {
      "types": "./dist/patterns/index.d.ts",
      "import": "./dist/patterns/index.js",
      "default": "./dist/patterns/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@a13y/core": "workspace:*"
  },
  "peerDependencies": {
    "@a13y/devtools": "^0.1.3",
    "react": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@a13y/devtools": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "dependencies": {
      "@a13y/core": "^0.1.3"
    }
  },
  "devDependencies": {
    "@a13y/devtools": "workspace:*",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "react": "^19.0.0",
    "tsup": "^8.5.1",
    "vitest": "^4.0.16"
  }
}
